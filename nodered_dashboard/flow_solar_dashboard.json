    {
        "id": "0473e229979cf97f",
        "type": "function",
        "z": "8f92054d3f48419c",
        "name": "Compute Efficiency",
        "func": "// assume panel area = 1.7 mÂ²\nconst AREA = 1.7;\n\n// extract values\nlet V = msg.payload.voltage;\nlet I = msg.payload.current;\nlet G = msg.payload.irradiance; // W/mÂ²\n\n// compute output & input power\nlet P_out = V * I;        // in Watts\nlet P_in  = G * AREA;     // in Watts\n\n// efficiency in percent\nlet eff = (P_in > 0) \n  ? (P_out / P_in) * 100 \n  : 0;\n\n// round to 1 decimal place\nmsg.payload = parseFloat(eff.toFixed(1));\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
